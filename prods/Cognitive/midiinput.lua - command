midiNoteOn(62)
midiNoteOff(62)

for i=30,60,1 do
  midiNoteOff(i)
end

clearTrace()
setMaxFramerate(100)

beginPlayback()
beginRecording()
stopRecorder()

print2(inspect(recorder.notes))
